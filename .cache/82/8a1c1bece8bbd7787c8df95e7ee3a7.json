__link {\n      font-size: 18px;\n      line-height: 21px;\n      letter-spacing: 0.0555556em;\n      text-transform: uppercase;\n    }\n\n    &__btn {\n\n      width: 180px;\n      height: 48px;\n      color: $color-white;\n      border: 2px solid $color-white;\n      letter-spacing: 0.05em;\n    }\n  }\n}\n\n\n// Banner\n.banner{\n  height: 1000px;\n  position: relative;\n  background: {\n    image: url(\"../img/Background.png\");\n    size: cover;\n    repeat: no-repeat;\n  }\n  mix-blend-mode: normal;\n  opacity: 0.8;\n  z-index: 1;\n\n  &::after {\n    content: \"\";\n    position: absolute;\n    left: 0; right: 0;\n    top: 0; bottom: 0;\n    background: $color-secondary;\n    mix-blend-mode: color;\n    z-index: -1;\n  }\n\n  &__container {\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    transform: translate(-50%,-50%);\n  }\n\n  &__content {\n    text-align: center;\n    margin-top: 20px;\n  }\n\n  &__form {\n    text-align: center;\n    margin-top: 50px;\n  }\n\n  &__email {\n    width: 270px;\n    height: 48px;\n    border: 1px solid $color-primary;\n    border-radius: 4px;\n    padding-left: 20px;\n    background-color: inherit;\n    margin-right: 15px;\n  }\n\n  &__btn {\n    border: 2px solid $color-primary;\n  }\n\n  &__footer {\n    display: flex;\n    justify-content: space-around;\n    max-width: 480px;\n    margin: auto;\n    margin-top: 100px;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"11ffc4f3eb52f194bbbfc84e84297446","cacheData":{"env":{}}}